package com.ayush;

import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int n = in.nextInt();
        int rem, ans = 0;

        while(n>0){
            rem = n % 10;
            n = n / 10;
            ans = ans  * 10 + rem;
        }
        System.out.println(ans);
    }
}

