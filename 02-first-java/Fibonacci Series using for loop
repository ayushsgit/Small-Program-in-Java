//To calculate Fibonacci Series up to n numbers.

package com.ayush;

import java.util.Scanner;

public class Main{
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        System.out.print("Enter the number till which you want to print fibonacci series : ");
        int n = input.nextInt();
        int a = 0, b = 1, c;
        System.out.print("fibonacci Series till " +n+ " is : ");

        for(int i = 1; i <= n; i++){
            System.out.print(a + ", ");
            c = a + b;
            a = b;
            b = c;
        }
    }
}


