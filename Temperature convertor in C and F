package com.ayush;

import java.util.Scanner;

public class Temperature {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        System.out.print("Please enter temp in C : ");
        double tempF,tempC;
        tempC = in.nextDouble();
        tempF = (tempC * 9/5) + 32;
        System.out.println("temp in F is : " + tempF);
    }
}


